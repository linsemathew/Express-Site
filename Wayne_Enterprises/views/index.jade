extends layout

block content
	//
		Carousel
		==================================================
	#myCarousel.carousel.slide(data-ride='carousel')
		// Indicators
		ol.carousel-indicators
			li.active(data-target='#myCarousel', data-slide-to='0')
			li(data-target='#myCarousel', data-slide-to='1')
			li(data-target='#myCarousel', data-slide-to='2')
		.carousel-inner(role='listbox')
			.item.active
				img.first-slide(src='../images/building.jpg', alt='First slide')
				.container
					.carousel-caption
						p
							a.btn.btn-lg.btn-primary(href='#', role='button') About Wayne Enterprises
			.item
				img.second-slide(src='../images/meeting.jpg', alt='Second slide')
				.container
					.carousel-caption
						p
							a.btn.btn-lg.btn-primary(href='#', role='button') Services
			.item
				img.third-slide(src='../images/contact.jpg', alt='Third slide')
				.container
					.carousel-caption
						p
							a.btn.btn-lg.btn-primary(href='#', role='button') Request A Consultation
		a.left.carousel-control(href='#myCarousel', role='button', data-slide='prev')
			span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
			span.sr-only Previous
		a.right.carousel-control(href='#myCarousel', role='button', data-slide='next')
			span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
			span.sr-only Next
	// /.carousel
	//
		Marketing messaging and featurettes
		==================================================
